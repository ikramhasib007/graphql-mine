{
  "name": "graphql-yoga-v2",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --exit-child --respawn src/index.ts",
    "migrate": "npx prisma migrate dev",
    "migrate:prod": "npm run db-clean && npx prisma migrate dev --create-only --name initial_migration && npm run deploy",
    "migrate-reset": "npx prisma migrate reset",
    "migrate-resolve": "npx prisma migrate resolve",
    "migrate-clean": "npm run db-clean && npx prisma migrate dev --name initial_migration",
    "generate": "npx prisma generate",
    "deploy": "npx prisma migrate deploy",
    "db-push": "npx prisma db push",
    "studio-dev": "npx prisma studio",
    "db-clean": "rimraf ./prisma/migrations",
    "postinstall": "graphql-codegen --config codegen.yml",
    "build": "rimraf ./build && tsc",
    "start": "npm run build && node build/index.js"
  },
  "license": "MIT",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.194.0",
    "@aws-sdk/lib-storage": "^3.194.0",
    "@envelop/core": "^3.0.2",
    "@graphql-tools/graphql-file-loader": "^7.5.5",
    "@graphql-tools/load": "^7.7.7",
    "@graphql-tools/load-files": "^6.6.1",
    "@graphql-yoga/node": "^2.13.13",
    "@paljs/plugins": "^4.1.0",
    "@prisma/client": "^4.5.0",
    "bcryptjs": "^2.4.3",
    "cuid": "^2.1.8",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.35",
    "mkdirp": "^1.0.4",
    "prisma-error-enum": "^0.1.3"
  },
  "devDependencies": {
    "@aws-sdk/abort-controller": "^3.201.0",
    "@faker-js/faker": "^7.6.0",
    "@graphql-codegen/cli": "^2.13.7",
    "@graphql-codegen/typescript": "^2.8.0",
    "@graphql-codegen/typescript-resolvers": "^2.7.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/formidable": "^2.0.5",
    "@types/mime-types": "^2.1.1",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^18.11.3",
    "cross-env": "^7.0.3",
    "prisma": "^4.5.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}
