{
  "name": "graphql-yoga-v3",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --exit-child --respawn src/index.ts",
    "check": "tsc --pretty --noEmit",
    "migrate": "npx prisma migrate dev",
    "migrate-reset": "npx prisma migrate reset",
    "migrate-resolve": "npx prisma migrate resolve",
    "migrate-clean": "npm run db-clean && npx prisma migrate dev --name initial_migration",
    "generate": "npx prisma generate",
    "deploy": "npx prisma migrate deploy",
    "db-push": "npx prisma db push",
    "studio-dev": "npx prisma studio",
    "db-clean": "rimraf ./prisma/migrations",
    "start": "ts-node src/index.ts",
    "postinstall": "graphql-codegen --config codegen.yml",
    "build": "rimraf ./dist && NODE_ENV=production tsc",
    "migrate:prod": "npm run db-clean && npx prisma migrate dev --create-only --name initial_migration && npm run deploy",
    "start:prod": "npm run build && node dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.600.0",
    "@aws-sdk/lib-storage": "^3.600.0",
    "@graphql-tools/load-files": "^7.0.0",
    "@paljs/plugins": "^7.0.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@whatwg-node/fetch": "^0.9.18",
    "bcryptjs": "^2.4.3",
    "graphql": "^16.9.0",
    "graphql-yoga": "^5.4.0",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "mkdirp": "^3.0.1",
    "prisma-error-enum": "^0.1.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.7",
    "@graphql-codegen/typescript-resolvers": "^4.1.0",
    "@prisma/client": "^5.16.0",
    "@smithy/abort-controller": "^3.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/formidable": "^3.4.5",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.14.9",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "prisma": "^5.16.0",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2"
  }
}
