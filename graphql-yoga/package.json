    "@aws-sdk/lib-storage": "^3.267.0",
    "@paljs/plugins": "^5.2.0",
    "@paralleldrive/cuid2": "^2.0.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.20.1",
    "graphql-yoga": "^3.5.1",
    "jsonwebtoken": "^9.0.0",
    "prisma-error-enum": "^0.1.3"
  },
  "devDependencies": {
    "@aws-sdk/abort-controller": "^3.267.0",
