    "@graphql-codegen/cli": "^3.0.0",
    "@prisma/client": "^4.10.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.13.0",
    "cross-env": "^7.0.3",
    "prisma": "^4.10.1",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
