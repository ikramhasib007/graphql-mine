{
  "name": "graphql-yoga-v3",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --exit-child --respawn src/index.ts",
    "check": "tsc --pretty --noEmit",
    "migrate": "npx prisma migrate dev",
    "migrate-reset": "npx prisma migrate reset",
    "migrate-resolve": "npx prisma migrate resolve",
    "migrate-clean": "npm run db-clean && npx prisma migrate dev --name initial_migration",
    "generate": "npx prisma generate",
    "deploy": "npx prisma migrate deploy",
    "db-push": "npx prisma db push",
    "studio-dev": "npx prisma studio",
    "db-clean": "rimraf ./prisma/migrations",
    "start": "ts-node src/index.ts",
    "postinstall": "graphql-codegen --config codegen.yml",
    "build": "rimraf ./dist && NODE_ENV=production tsc",
    "migrate:prod": "npm run db-clean && npx prisma migrate dev --create-only --name initial_migration && npm run deploy",
    "start:prod": "npm run build && node dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.279.0",
    "@aws-sdk/lib-storage": "^3.279.0",
    "@graphql-tools/graphql-file-loader": "^7.5.16",
    "@graphql-tools/load": "^7.8.12",
    "@graphql-tools/load-files": "^6.6.1",
    "@paljs/plugins": "^5.2.0",
    "@paralleldrive/cuid2": "^2.2.0",
    "@whatwg-node/fetch": "^0.8.1",
    "bcryptjs": "^2.4.3",
    "graphql": "^16.6.0",
    "graphql-yoga": "^3.7.0",
    "jsonwebtoken": "^9.0.0",
    "mime-types": "^2.1.35",
    "mkdirp": "^2.1.3",
    "prisma-error-enum": "^0.1.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@aws-sdk/abort-controller": "^3.272.0",
    "@faker-js/faker": "^7.6.0",
    "@graphql-codegen/cli": "^3.2.1",
    "@graphql-codegen/typescript": "^3.0.1",
    "@graphql-codegen/typescript-resolvers": "^3.1.0",
    "@prisma/client": "^4.10.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/formidable": "^2.0.5",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mime-types": "^2.1.1",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^18.14.1",
    "@types/uuid": "^9.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "prisma": "^4.10.1",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  }
}
